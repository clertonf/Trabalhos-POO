package agendacontato;
public class telefone{
    
    String foneid;
    int numero;
    
    // CONSTRUTOR : É ELE QUE INSTANCIA(COLOCA VALORES A CLASSE;
   
    public telefone(String foneid , int numero){
        this.foneid = foneid;
        this.numero = numero;
        
      }
    //Essa classe toString é utilizada apenas para retornar
    //as informações de classe de maneira organizada e
    //facilitar futuros prints;
    @Override
    public String toString(){
        return "["+foneid+":"+numero+"]";
     }
}
